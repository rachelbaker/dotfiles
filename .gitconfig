[core]
	autocrlf = input
	excludesfile = ~/.gitignore
    filemode = false
[user]
	name = Rachel Baker
	email = rbaker@nerdery.com
[color]
	ui = true
[alias]
 br = branch
 cm = commit
 co = checkout
 df = diff
 g = grep -I
 lg = log -p
 loq = log --graph --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold white)â€” %an%C(reset)%C(bold yellow)%d%C(reset)' --abbrev-commit --date=relative
 pb = publish-branch
 pp = !"echo 'Pull'; git pull; echo 'Push'; git push"
 rv = review
 sm = show-merges
 st = status
[credential]
	helper = store
[push]
	default = simple
[mergetool]
    keepBackup = true